@article{ ISI:000407360600004,
Author = {Lu, Xiaofei and Wang, Ben Pin-Yun},
Title = {{Towards a metaphor-annotated corpus of Mandarin Chinese}},
Journal = {{LANGUAGE RESOURCES AND EVALUATION}},
