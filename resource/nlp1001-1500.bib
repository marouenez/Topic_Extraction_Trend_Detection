@article{ ISI:000332947200003,
Author = {Iemmolo, Giorgio and Arcodia, Giorgio Francesco},
Title = {{Differential Object Marking and identifiability of the referent: A study
   of Mandarin Chinese}},
Journal = {{LINGUISTICS}},
Year = {{2014}},
Volume = {{52}},
Number = {{2}},
Pages = {{315-334}},
Month = {{MAR}},
Abstract = {{This paper examines the interaction of DOM with information structure in
   Mandarin Chinese. Despite the large amount of works on this topic, much
   remains to be explained, in particular with respect to some alternations
   that do not easily fit the explanations proposed so far in terms of
   affectedness, animacy and definiteness. Through the analysis of text
   excerpts taken from the Corpus of Modern Chinese of the Center for
   Chinese Linguistics (CCL) of Peking University, we argue that, in
   addition to previously identified constraints, DOM in Mandarin Chinese
   performs another important function in discourse, namely that of
   signalling the high identifiability of the marked referent.}},
