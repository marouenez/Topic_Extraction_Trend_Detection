@article{ ISI:000399089600007,
Author = {Meo, Rosa and Sulis, Emilio},
Title = {{Processing Affect in Social Media: A Comparison of Methods to
   Distinguish Emotions in Tweets}},
Journal = {{ACM TRANSACTIONS ON INTERNET TECHNOLOGY}},
Year = {{2017}},
Volume = {{17}},
Number = {{1, SI}},
Month = {{MAR}},
Abstract = {{Emotion analysis in social media is challenging. While most studies
   focus on positive and negative sentiments, the differentiation between
   emotions is more difficult. We investigate the problem as a collection
   of binary classification tasks on the basis of four opposing emotion
   pairs provided by Plutchik. We processed the content of messages by
   three alternative methods: structural and lexical features, latent
   factors, and natural language processing. The final prediction is
   suggested by classifiers deriving from the state of the art in machine
   learning. Results are convincing in the possibility to distinguish the
   emotions pairs in social media.}},
