@article{ ISI:000324386000005,
Author = {Zitek, Andreas and Poppe, Michaela and Stelzhammer, Michael and Muhar,
   Susanne and Bredeweg, Bert},
Title = {{Learning by Conceptual Modeling-Changes in Knowledge Structure and
   Content}},
