@article{ ISI:000262688100007,
Author = {Fattah, Mohamed Abdel and Ren, Fuji},
Title = {{GA, MR, FFNN, PNN and GMM based models for automatic text summarization}},
Journal = {{COMPUTER SPEECH AND LANGUAGE}},
