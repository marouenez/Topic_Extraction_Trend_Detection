@article{ ISI:000325991600005,
Author = {Choo, Jaegul and Lee, Changhyun and Reddy, Chandan K. and Park, Haesun},
Title = {{UTOPIAN: User-Driven Topic Modeling Based on Interactive Nonnegative
   Matrix Factorization}},
Journal = {{IEEE TRANSACTIONS ON VISUALIZATION AND COMPUTER GRAPHICS}},
