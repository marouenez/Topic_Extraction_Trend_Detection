@article{ ISI:000220595100003,
Author = {Schleppegrell, MJ and Achugar, M and Oteiza, T},
Title = {{The grammar of history: Enhancing content-based instruction through a
   functional focus on language}},
Journal = {{TESOL QUARTERLY}},
Year = {{2004}},
Volume = {{38}},
Number = {{1}},
Pages = {{67-93}},
Month = {{SPR}},
Abstract = {{In K-12 contexts, the teaching of English language learners (ELLs) has
   been greatly influenced by the theory and practice of content-based
   instruction (CBI). A focus on content can help students achieve
   grade-level standards in school subjects while they develop English
   proficiency, but CBI practices have focused primarily on vocabulary and
   the use of graphic organizers along with cooperative learning
   activities. This article reports the results of a project intended to
   enhance CBI through activities that focus on the role of language in
   constructing knowledge. The strategies we present are based on
   identification and analysis of the challenges presented by grade-level
   textbooks in middle school history classrooms. By engaging in functional
   linguistic analysis, ELLs and their teachers can deconstruct the
   language of their textbooks, enabling students to develop academic
   language by focusing on the meaning-making potential of the historian's
   language choices.}},
