@article{ ISI:000261303800004,
Author = {Drouin, Patrick},
Title = {{Automatic identification of transdisciplinary scientific lexicon}},
Journal = {{REVUE FRANCAISE DE LINGUISTIQUE APPLIQUEE}},
Year = {{2007}},
Volume = {{12}},
Number = {{2}},
Pages = {{45-64}},
Month = {{DEC}},
Abstract = {{In this paper, we propose a first step leading to the description of the
   lexicon of scientific language by identifying a transdisciplinary
   scientific lexicon (TSL). The TSL is domain independent and forms a
   central lexical core of all domains; it is at the center of
   argumentation we find in scientific discourse as well as in its
   structuring. In order to gather the transdisciplinary lexicon, we use
   natural language processing (NLP) tools and statistical techniques;
   central to our method is the calcul des specificites (specificity
   measure) put forward by Lafon (1980). By using NLP tools, we want to
   verify if it is possible to quickly and simply complement existing
   lexical resources without much manual intervention. We conclude our
   study by exploring an observation made by Phal (1971) about collocations
   and scientific discourse. We focus here on V-N collocations revolving
   around nouns taken from our TSL.}},
