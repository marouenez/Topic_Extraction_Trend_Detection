@article{ ISI:000250455100004,
Author = {Zinken, Joerg},
Title = {{Discourse metaphors: The link between figurative language and habitual
   analogies}},
Journal = {{COGNITIVE LINGUISTICS}},
Year = {{2007}},
Volume = {{18}},
Number = {{3}},
Pages = {{445-466}},
Abstract = {{Cognitive linguists have long been interested in analogies people
   habitually use in thinking and speaking, but little is known about the
   nature of the relationship between verbal behaviour and such analogical
   schemas. This article proposes that discourse metaphors are an important
   link between the two. Discourse metaphors are verbal expressions
   containing a construction that evokes an analogy negotiated in the
   discourse community. Results of an analysis of metaphors in a corpus of
   newspaper texts support the prediction that regular analogies are
   form-specific, i.e., bound to particular lexical items. Implications of
   these results for assumptions about the generality of habitual analogies
   are discussed.}},
