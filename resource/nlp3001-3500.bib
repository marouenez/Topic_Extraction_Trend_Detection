@article{ ISI:000259595700009,
Author = {Perri, Antonio},
Title = {{Afterword: Reading without spirit?}},
Journal = {{TEXT \& TALK}},
Year = {{2008}},
Volume = {{28}},
Number = {{5, SI}},
Pages = {{691-698}},
Abstract = {{In this article I provide a short critical review of the main
   contributions in this Text \& Talk special issue. This afterword offers
   an anthropologically oriented reassessment of traditional views of
   writing and reading. After a discussion of some basic concepts such as
   readability, legibility and visibility, a new model of the functioning
   of reading scripts is mentioned-a bascially context-grounded,
   interpretive, and situated perspective. From the analysis of
   cross-cultural settings and situations provided by the articles in the
   issue, we learn that a new, flexible idea of reading as an integrated
   practice dismissing the notion of text as a given, context-free written
   sign structure can also be a key tool for a better understanding of
   contemporary communicative frameworks.}},
