@article{ ISI:000283836700004,
Author = {Van de Cruys, Tim},
Title = {{A non-negative tensor factorization model for selectional preference
   induction}},
Journal = {{NATURAL LANGUAGE ENGINEERING}},
Year = {{2010}},
Volume = {{16}},
Number = {{4, SI}},
Pages = {{417-437}},
Month = {{OCT}},
Abstract = {{The distributional similarity methods have proven to be a valuable tool
   for the induction of semantic similarity. Until now, most algorithms use
   two-way co-occurrence data to compute the meaning of words.
   Co-occurrence frequencies, however, need not be pairwise. One can easily
   imagine situations where it is desirable to investigate co-occurrence
   frequencies of three modes and beyond. This paper will investigate
   tensor factorization methods to build a model of three-way
   co-occurrences. The approach is applied to the problem of selectional
   preference induction, and automatically evaluated in a
   pseudo-disambiguation task. The results show that tensor factorization,
   and non-negative tensor factorization in particular, is a promising tool
   for Natural Language Processing (NLP).}},
