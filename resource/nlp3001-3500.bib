@article{ ISI:000275882500001,
Author = {Kataoka, Kuniyoshi},
Title = {{A multi-modal ethnopoetic analysis (Part 2): Catchment, prosody, and
   frames of reference in Japanese spatial narrative}},
Journal = {{LANGUAGE \& COMMUNICATION}},
Year = {{2010}},
Volume = {{30}},
Number = {{2}},
Pages = {{69-89}},
Month = {{APR}},
Abstract = {{In this paper, I examine the interplay of language-specific factors and
   socio-cognitive motivations for the poetic construction of language and
   gesture in Japanese spatial narrative. By employing an ethnopoetic
   approach and analyzing the text, the body, and the environment in
   conjunction with the thematic cohesion via ``catchment{''} (McNeill, D.,
   2004. Gesture and Thought. The University of Chicago Press, Chicago), I
   argue that speakers coordinately cue subtle shifts of the narrative
   phases in terms of forms, referents, and shifts of hand gestures. Based
   on this argument, I claim that not only linguistic but also multi-modal
   semiotic resources can substantially contribute to the achievement of
   holistic poetic configuration and an intersubjective schema. for
   narrative comprehension. (C) 2009 Elsevier Ltd. All rights reserved.}},
