@article{ ISI:000277785500007,
Author = {Georgiou, Vasiliki},
Title = {{Competing discourses in the debate on place names in Cyprus Issues of
   (symbolic) inclusion/exclusion in orthographic choices}},
Journal = {{JOURNAL OF LANGUAGE AND POLITICS}},
Year = {{2010}},
Volume = {{9}},
