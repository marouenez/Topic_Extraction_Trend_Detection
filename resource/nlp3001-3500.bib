@article{ ISI:000285138200007,
Author = {Prado-Alonso, Carlos and Carlos Acuna-Farina, J.},
Title = {{A comprehensive account of full-verb inversion in English}},
Journal = {{FOLIA LINGUISTICA}},
Year = {{2010}},
Volume = {{44}},
Number = {{2}},
Pages = {{509-554}},
Month = {{DEC}},
Abstract = {{Despite the abundant corpus-based research on full-inversion
   constructions in which the subject follows the entire verb phrase in a
   declarative clause, the analysis of this construction in the spoken mode
   has traditionally been neglected since it has been suggested that
   natural speech is not a good source of inversions. The article shows
   that speech, too, can be a good source of data for full inversion and
   that the spoken and the written modes do not differ in the overall
   distribution of the construction but rather in the types of inversion
   used and in the pragmatic functions these inversions serve in discourse.
   The article also casts light on the lack of consensus regarding the
   distribution of full inversion in written fictional and non-fictional
   texts, and in the pragmatic function the construction serves in both
   genres.}},
