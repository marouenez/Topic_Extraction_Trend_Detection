@article{ ISI:000391928300003,
Author = {Epps, Patience L. and Webster, Anthony K. and Woodbury, Anthony C.},
Title = {{A HOLISTIC HUMANITIES OF SPEAKING: FRANZ BOAS AND THE CONTINUING
   CENTRALITY OF TEXTS}},
Journal = {{INTERNATIONAL JOURNAL OF AMERICAN LINGUISTICS}},
Year = {{2017}},
Volume = {{83}},
Number = {{1}},
Pages = {{41-78}},
Month = {{JAN}},
Abstract = {{We take up Boas's commitment to the establishment of a large corpus of
   texts from the indigenous languages and peoples of the Americas,
   examining what we take to be his fundamental principles: using texts as
   the philological record from which to document and explore language,
   culture, and intellectual life on their own terms; training speakers to
   engage in documentation through the creation and analysis of texts; and
   a focus on the emergence of patterns and interrelationships. We then
   outline what we see as his influence up to now, both directly through
   the kinds of projects he promoted and indirectly through a broader
   application of Boasian principles that has animated a series of later
   movements. Finally, we discuss the prospects for a more comprehensive
   text-and documentation-centered approach to language and culture that
   welds together these themes and movements, and which we envision as a
   holistic humanities of speaking.}},
