@article{ ISI:000321607200004,
Author = {Pavlyshenko, Bohdan},
Title = {{Classification analysis of authorship fiction texts in the space of
   semantic fields}},
Journal = {{JOURNAL OF QUANTITATIVE LINGUISTICS}},
Year = {{2013}},
Volume = {{20}},
Number = {{3}},
Pages = {{218-226}},
Month = {{AUG 1}},
Abstract = {{The use of naive Bayesian classifier (NB) and the classifier by the k
   nearest neighbours (kNN) in a classification semantic analysis of
   authors' texts of English fiction has been analyzed. The authors' works
   are considered in the vector space the basis of which is formed by the
   frequency characteristics of semantic fields of nouns and verbs. Highly
   precise classification of authors' texts in the vector space of semantic
   fields indicates the presence of particular spheres of the author's
   idiolect in this space which characterizes the individual author's
   style.}},
