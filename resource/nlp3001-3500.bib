@article{ ISI:000376440000007,
Author = {Hansen, Julie},
Title = {{Translating the translingual text Olga Grushin's anglophone novel The
   Dream Life of Sukhanov in Russian}},
Journal = {{TRANSLATION AND INTERPRETING STUDIES}},
