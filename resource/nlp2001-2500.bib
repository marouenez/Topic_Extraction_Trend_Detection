@article{ ISI:000407573100003,
Author = {Beloki, Zuhaitz and Artola, Xabier and Soroa, Aitor},
Title = {{A scalable architecture for data-intensive natural language processing}},
Journal = {{NATURAL LANGUAGE ENGINEERING}},
