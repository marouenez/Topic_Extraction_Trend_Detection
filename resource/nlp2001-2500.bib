@article{ ISI:000295869300006,
Author = {Newfield, Denise},
Title = {{From visual literacy to critical visual literacy: An analysis of
   educational materials}},
Journal = {{ENGLISH TEACHING-PRACTICE AND CRITIQUE}},
Year = {{2011}},
Volume = {{10}},
Number = {{1}},
Pages = {{81-94}},
Month = {{MAY}},
Abstract = {{This article discusses differences in purpose, orientation and method
   between what is commonly known as ``visual literacy{''} and what is
   being called ``critical visual literacy{''}. It does so through a
   comparative critical analysis of two sets of materials produced for
   classroom use: those produced in 1993 under the umbrella of visual
   literacy and those produced in 2011 under the umbrella of critical
   visual literacy. Through an examination of different approaches to
   context, semiotic choice and authorial discourse in the development of
   the material, the article shows the distinctive nature of critical
   visual literacy - its emphasis on the positioned and positioning nature
   of visual texts, on the socio-political consequences of semiotic choice
   in visual texts, and on reading against rather than reading with the
   visual text.}},
