@article{ ISI:000402072700007,
Author = {Al-Ayyoub, Mahmoud and Jararweh, Yaser and Rabab'ah, Abdullateef and
   Aldwairi, Monther},
Title = {{Feature extraction and selection for Arabic tweets authorship
   authentication}},
Journal = {{JOURNAL OF AMBIENT INTELLIGENCE AND HUMANIZED COMPUTING}},
Year = {{2017}},
