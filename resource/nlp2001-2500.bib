@article{ ISI:000394450200002,
Author = {Chen, Yumin},
Title = {{Reconstructing classical Chinese fables through modern adaptations: a
   functional comparative study}},
Journal = {{SOCIAL SEMIOTICS}},
Year = {{2017}},
Volume = {{27}},
Number = {{2}},
Pages = {{145-157}},
Abstract = {{This paper analyses and compares classical Chinese fables and their
   modern adaptations in the reconstruction of experiential meaning and
   mediation of attitudinal stance. Drawing upon systemic functional
   linguistics with a particular focus on transitivity, projection and
   appraisal, the analysis shows that the adapted texts expand the original
   in various ways. Experientially the adaptations expand the classical
   fables through an enhanced description of the possible causes of the
   significant plot. Furthermore, the adapted dialogues make visible the
   communicative addressee and enable wider variations in mood.
   Interpersonally the adapted attitudinal realizations demonstrate
   variations in terms of the types of attitude and the way to encode
   attitudinal meanings. It further explores the reasons for the
   differences between the original and adapted texts in view of the
   historical development from classical Chinese to modern vernacular as
   well as the needs of the audience. The findings may shed light on how
   social and semiotic developments could exert influence on the modern
   reconstruction of classical texts.}},
