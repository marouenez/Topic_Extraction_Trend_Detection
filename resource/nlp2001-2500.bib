@article{ ISI:000312465300004,
Author = {Cormier, Kearsy and Quinto-Pozos, David and Sevcikova, Zed and Schembri,
   Adam},
Title = {{Lexicalisation and de-lexicalisation processes in sign languages:
   Comparing depicting constructions and viewpoint gestures}},
Journal = {{LANGUAGE \& COMMUNICATION}},
Year = {{2012}},
Volume = {{32}},
Number = {{4}},
Pages = {{329-348}},
Month = {{OCT}},
Abstract = {{In this paper, we compare so-called ``classifier{''} constructions in
   signed languages (which we refer to as ``depicting constructions{''})
   with comparable iconic gestures produced by non-signers. We show clear
   correspondences between entity constructions and observer viewpoint
   gestures on the one hand, and handling constructions and character
   viewpoint gestures on the other. Such correspondences help account for
   both lexicalisation and de-lexicalisation processes in signed languages
   and how these processes are influenced by viewpoint. Understanding these
   processes is crucial when coding and annotating natural sign language
   data. (C) 2012 Elsevier Ltd. All rights reserved.}},
