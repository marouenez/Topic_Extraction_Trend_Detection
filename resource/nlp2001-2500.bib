@article{ ISI:000395128000004,
Author = {Alyousef, Hesham Suleiman and Alsharif, Amerah Abdullah},
Title = {{The Experiential Meaning in Saudi Postgraduate Business Students'
   Multimodal Accounting Texts: A Multidimensional Exploration}},
Journal = {{AUSTRALIAN JOURNAL OF LINGUISTICS}},
