@article{ ISI:000260646200006,
Author = {Seidel, Frank},
Title = {{The hodiernal past domain and the concept of recentness in Yeyi}},
Journal = {{AFRICANA LINGUISTICA}},
Year = {{2008}},
Volume = {{14}},
Pages = {{151-176}},
Abstract = {{This article deals with functions and semantics of Yeyi (R41) past verb
   tenses against the background of the temporal concepts of a prehodiernal
   past and a hodiernal past. More specifically, it addresses the fact that
   in Yeyi the past verb tenses are ordered around and interact with a
   temporal cut-off point that is located one full cycle of day and night
   (roughly 24 hours) back in time from the moment of speech. This cut-off
   point, as will be argued on the basis of a detailed text analysis, is
   the result of the interaction of two concepts: the hodiernality of an
   event and the recentness of an event. The temporal aspects of the Yeyi
   language will be presented following Botne and Kershner's (2006)
   cognitive framework for the organisation of tense systems.}},
