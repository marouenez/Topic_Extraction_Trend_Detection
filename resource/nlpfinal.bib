@article{ ISI:000273849400004,
Author = {Cantos, Pascual and Sanchez, Aquilino and Almela, Moises},
Title = {{AN ATTEMPT TO FORMALIZE WORD SENSE DISAMBIGUATION: MAXIMIZING EFFICIENCY
   BY MINIMIZING COMPUTATIONAL COSTS}},
Journal = {{REVISTA ESPANOLA DE LINGUISTICA APLICADA}},
Year = {{2009}},
Volume = {{22}},
Pages = {{77-88}},
Abstract = {{This paper presents an algorithm based on collocational data for word
   sense disambiguation (WSD). Vie aim of this algorithm is to maximize
   efficiency by minimizing (1) computational costs and (2) linguistic
   tagging/annotation. The formalization of our WSD algorithm is based on
   discriminant function analysis (DFA). This statistical technique allows
   its to parameterize each collocational item with its meaning, using Just
   hare text, The parameterized data allow its to classify cases (sentences
   with art ambiguous word) into the values of a categorical dependent
   (each of the meanings of the ambiguous word). To evaluate the validity
   and efficiency of our WSD algorithm, we previously hand sense-tagged all
   the sentences containing ambiguous words and then cross-validated the
   hand sense-tagged data with the automatic WSD performance. Finally, we
   present the global results of our algorithm after applying it to a
   limited set of words in both languages: Spanish and English,
   highlighting the points we consider relevant for further analysis.}},
