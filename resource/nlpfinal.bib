@article{ ISI:000417904700002,
Author = {Alex, Beatrice and Grover, Claire and Oberlander, Jon and Thomson, Tara
   and Anderson, Miranda and Loxley, James and Hinrichs, Uta and Zhou, Ke},
Title = {{Palimpsest: Improving assisted curation of loco-specific literature}},
