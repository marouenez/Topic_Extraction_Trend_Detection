@article{ ISI:A1997XY63900005,
Author = {ElShiyab, S},
Title = {{Lexical cohesion with reference to the identity chain: Application of
   (IC) to different types of Arabic texts}},
Journal = {{IRAL-INTERNATIONAL REVIEW OF APPLIED LINGUISTICS IN LANGUAGE TEACHING}},
Year = {{1997}},
Volume = {{35}},
Number = {{3}},
Pages = {{211-223}},
Month = {{AUG}},
Abstract = {{This paper is a textual and an analytical description of texts. That is,
   it describes the identity chain manifested in different types of Arabic
   texts. An attempt will be made to demonstrate the ways and means through
   which a chain is formulated and constructed. An attempt will also be
   made to highlight the functions performed by such chains.
   Moreover, emphasis will be placed on chain interactions. That is, the
   question whether chain or chain interactions are significant for judging
   textual coherence will be answered.
   Based on the analysis of texts, it was found that chains are unifying
   relations; they are realized through referential and lexical cohesive
   devices. These devices create texture and structure within texts.}},
