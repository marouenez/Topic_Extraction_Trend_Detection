@article{ ISI:000392701200008,
Author = {Perez-Salazar, Carmela},
Title = {{Proclamations and banns: Written tradition and oral transmission in
   texts of authority}},
Journal = {{Circulo de Linguistica Aplicada a la Comunicacion}},
Year = {{2016}},
Number = {{68}},
Pages = {{253-294}},
Abstract = {{Proclamations and banns: Written tradition and oral transmission in
   texts of authority. Aloud transmission of news has taken place from the
   Middle Ages until present. This article describes the transformations
   undergone by the discursive tradition known in Spanish as pregon or
   bando (announcement or proclamation). On the one hand we examine the
   information about announcements and proclamations included in
   lexicographic sources over time; and on the other hand we develop a
   diachronic analysis -based on literary and non-literary documents- of
   the peculiar tension between orality and literacy, the
   diplomatic-textual structure and the deictic devices that characterize
   this discursive tradition.}},
