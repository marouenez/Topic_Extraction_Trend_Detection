@article{ ISI:000072558200002,
Author = {Okada, S},
Title = {{On the parallelism between sentence-formation and text-formation with
   special reference to coordinate structures}},
Journal = {{LINGUA}},
Year = {{1997}},
Volume = {{103}},
Number = {{4}},
Pages = {{225-254}},
Month = {{DEC}},
Abstract = {{In this paper, we will demonstrate the following three points: (1) that
   the Coordinate Structure Constraint (CSC) proposed by Ross (1967/1986)
   is untenable as a syntactic constraint and should be replaced by a
   functional constraint based on the claim of Kuno (1976), and that the
   restrictions on Across-The-Board (ATE) extractions should also be
   reconsidered from a semantic point of view (a questionnaire was carried
   out and the results were subjected to statistical methods to support the
   present claims); (2) that coordinate structures within relative clauses
   form a kind of semantic closure (to be illustrated by way of simple
   schemata) describing a fixed theme, which is the antecedent; (3) and
   that the notion of theme/theme, which is often considered to be a part
   of the exclusive domain of discourse and text analysis, is projected
   down to the sentence-level to regulate the relation of the antecedent
   with the rest of a relative.}},
