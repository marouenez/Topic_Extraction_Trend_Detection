@article{ ISI:000081114000002,
Author = {Bruce, RF and Wiebe, JM},
Title = {{Decomposable modeling in natural language processing}},
Journal = {{COMPUTATIONAL LINGUISTICS}},
Year = {{1999}},
Volume = {{25}},
Number = {{2}},
Pages = {{195-207}},
Month = {{JUN}},
Abstract = {{In this paper, we describe a framework for developing probabilistic
   classifiers in natural language processing. Our focus is on formulating
   models that capture the most important interdependencies among features,
   to avoid overfitting the data while also characterizing the data well.
   The class of probability models and the associated inference techniques
   described here were developed in mathematical statistics, and are widely
   used in artificial intelligence and applied statistics. Our goal is to
   make this model selection framework accessible to researchers in NLP,
   and provide pointers to available software and important references. In
   addition, toe describe how the quality of the three determinants of
   classifier performance (the features, the form of the model, and the
   parameter estimates) can be separately evaluated. We also demonstrate
   the classification performance of these models in a large-scale
   experiment involving the disambiguation of 34 words taken from the
   HECTOR word sense corpus (Hanks 1996). In 10-fold cross-validations, the
   model search procedure performs significantly better than naive Bayes on
   6 of the words without being significantly worse on any of them.}},
