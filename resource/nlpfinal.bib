@article{ ISI:000399404700002,
Author = {Qin, Zhen and Chien, Yu-Fu and Tremblay, Annie},
Title = {{Processing of word-level stress by Mandarin-speaking second language
   learners of English}},
Journal = {{APPLIED PSYCHOLINGUISTICS}},
Year = {{2017}},
Volume = {{38}},
Number = {{3}},
Pages = {{541-570}},
Month = {{MAY}},
Abstract = {{This study investigates whether second language learners' processing of
   stress can be explained by the degree to which suprasegmental cues
   contribute to lexical identity in the native language. It focuses on
   Standard Mandarin, Taiwan Mandarin, and American English listeners'
   processing of stress in English nonwords. In Mandarin, fundamental
   frequency contributes to lexical identity by signaling lexical tones,
   but only in Standard Mandarin does duration distinguish
   stressed-unstressed and stressed-stressed words. Participants completed
   sequence-recall tasks containing English disyllabic nonwords contrasting
   in stress. Experiment 1 used natural stimuli; Experiment 2 used
   resynthesized stimuli that isolated fundamental frequency and duration
   cues. Experiment 1 revealed no difference among the groups; in
   Experiment 2, Standard Mandarin listeners used duration more than Taiwan
   Mandarin listeners did. These results are interpreted within a
   cue-weighting theory of speech perception.}},
