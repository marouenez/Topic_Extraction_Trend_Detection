@article{ ISI:000272533900021,
Author = {de Schryver, Gilles-Maurice},
Title = {{Lexikos at Eighteen: An Analysis}},
Journal = {{LEXIKOS}},
Year = {{2009}},
Volume = {{19}},
Pages = {{372-403}},
Abstract = {{At eighteen, Lexikos became a major player in the field of linguistics,
   by being awarded an Impact Factor. This article presents a double
   analysis of the foundation that led to this success. On the one hand a
   thorough statistical study is undertaken with regard to all contributors
   and their contributions to Lexikos. To this end a metadata database was
   designed, with the aim to answer the question: `Who publishes what type
