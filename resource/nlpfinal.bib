@article{ ISI:000272706900004,
Author = {van der Slik, Frans},
Title = {{Gender bias and gender differences in two South African tests of
   academic literacy}},
Journal = {{SOUTHERN AFRICAN LINGUISTICS AND APPLIED LANGUAGE STUDIES}},
Year = {{2009}},
Volume = {{27}},
Number = {{3}},
Pages = {{277-290}},
Month = {{NOV}},
Abstract = {{How much empirical evidence is there of gender bias in the Test of
   Academic Literacy Levels (TALL) and its Afrikaans counterpart, Toets van
   Akademiese Geletterdheidsvlakke (TAG)? This paper reports on
   meta-analyses of 24 academic tests, containing data of more than 60 000
   first-year South African students. TALL appears to be gender-neutral,
   while for TAG some evidence was found that men slightly outperform
   women. This outcome can be attributed almost entirely to male students
   outperforming female students on the subtest requiring the
   interpretation of graphs and visual information. Women outperform men
   slightly on the subtest dealing with text types. These outcomes are
   presented in light of discussions on gender differences versus gender
   bias.}},
