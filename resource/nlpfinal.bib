@article{ ISI:000417904700006,
Author = {Giovannetti, Emiliano and Albanesi, Davide and Bellandi, Andrea and
   Benotto, Giulia},
Title = {{Traduco: A collaborative web-based CAT environment for the
   interpretation and translation of texts}},
