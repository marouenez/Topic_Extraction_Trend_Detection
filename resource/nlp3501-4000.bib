@article{ ISI:000173229000001,
Author = {Callahan, L},
Title = {{The matrix language frame model and Spanish/English codeswitching in
   fiction}},
Journal = {{LANGUAGE \& COMMUNICATION}},
Year = {{2002}},
Volume = {{22}},
Number = {{1}},
Pages = {{1-16}},
Month = {{JAN}},
Abstract = {{The Matrix Language Frame (MLF) model of structural constraints on
   codeswitching was presented by Carol Myers-Scotton in her 1993 book
   Duelling Languages (Myers-Scotton, C., 1993. Duelling languages.
   Clarendon Press, Oxford). This article reports on a test of the model
   using a written corpus. The corpus consists of novels and short stories
   containing Spanish/English codeswitching published in the United States
   between 1970-2000. The analysis reveals relatively few counterexamples
   to the MLF models the majority of codeswitches found in these texts can
   be accounted For by Myers-Scotton's model of grammatical structure. The
   application of a model formulated for speech to a written corpus has
   implications for the relationship between spoken and written data and
   the use of the latter in linguistic analyses. The most salient feature
   of the data is a high incidence of Matrix Language (ML) shifts at
   mid-sentence. Modifications to the original exposition of the model are
   proposed in regard to the flexibility of the ML and the constituent
   types possible. (C) 2002 Elsevier Science Ltd. All rights reserved.}},
