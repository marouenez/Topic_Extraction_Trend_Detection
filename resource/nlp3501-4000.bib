@article{ ISI:000295305000001,
Author = {Prillop, Kuelli},
Title = {{FINNIC STOP GRADATION AS AN EFFECT OF MORA SHARING}},
Journal = {{LINGUISTICA URALICA}},
Year = {{2011}},
Volume = {{47}},
Number = {{3}},
Pages = {{161-181}},
Abstract = {{Finnic radical stop gradation affected stop consonants in the onset
   position of unstressed syllables. The stops were weakened before closed
   syllables, and remained unweakened before open syllables. Stop
   gradation, conditioned by whether the following syllable was open or
   closed, is cross-linguistically extremely rare. In this paper, gradation
   is argued to be a phonetically natural intervocalic weakening process,
   which was blocked only in the onsets of open unstressed syllables due to
   restrictions on moraic structure. Under the proposed account,
   differences between Finnic languages in terms of gradation depend on the
   extent to which a specific language tolerated heavy unstressed
   syllables.}},
