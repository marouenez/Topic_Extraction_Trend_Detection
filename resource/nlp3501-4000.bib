@article{ ISI:A1994QA45400001,
Author = {BIEBOW, B and SZULMAN, S},
Title = {{ACQUISITION AND VALIDATION OF SOFTWARE REQUIREMENTS}},
Journal = {{KNOWLEDGE ACQUISITION}},
