@article{ ISI:000165128200002,
Author = {Laguitton, V and De Graaf, JB and Chauvel, P and Liegeois-Chauvel, C},
Title = {{Identification reaction times of voiced/voiceless continua: A right-ear
   advantage for VOT values near the phonetic boundary}},
Journal = {{BRAIN AND LANGUAGE}},
Year = {{2000}},
Volume = {{75}},
Number = {{2}},
Pages = {{153-162}},
Month = {{NOV}},
Abstract = {{We explored the degree to which the duration of acoustic cues
   contributes to the respective involvement of the two hemispheres in the
   perception of speech. To this end, we recorded the reaction time needed
   to identify monaurally presented natural French plosives with varying
   VOT values. The results show that a right-ear advantage is significant
   only when the phonetic boundary is close to the release burst, i.e.,
   when the identification of the two successive acoustical events (the
   onset of voicing and the release from closure) needed to perceive a
   phoneme as voiced or voiceless requires rapid information processing.
   These results are consistent with the recent hypothesis that the left
   hemisphere is superior in the processing of rapidly changing acoustical
   information. (C) 2000 Academic Press.}},
