@article{ ISI:000089894900007,
Author = {Harabagiu, SM},
Title = {{Patterns of prepositional attachments - Where dictionary semantics meets
   corpus statistics}},
Journal = {{INTERNATIONAL JOURNAL OF PATTERN RECOGNITION AND ARTIFICIAL INTELLIGENCE}},
Year = {{2000}},
Volume = {{14}},
Number = {{6}},
Pages = {{809-838}},
Month = {{SEP}},
Abstract = {{This paper presents a novel methodology of disambiguating prepositional
   phrase attachments. We create patterns of attachments by classifying a
   collection of prepositional relations derived from Treebank parses. As a
   by-product, the arguments of every prepositional relation are
   semantically disambiguated. Attachment decisions are generated as the
   result of a learning process, that builds upon some of the most popular
   current statistical and machine learning techniques.
   We have tested this methodology on (1) Wall Street Journal articles, (2)
   textual definitions of concepts from a dictionary and (3) an ad hoc
   corpus of Web documents, used for conceptual indexing and information
   extraction.}},
