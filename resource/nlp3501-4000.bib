@article{ ISI:A1994NH59000003,
Author = {KENNEDY, VR},
Title = {{PICTURES AS METAPHORS IN THACKERAY ILLUSTRATED NOVELS}},
Journal = {{METAPHOR AND SYMBOLIC ACTIVITY}},
Year = {{1994}},
Volume = {{9}},
Number = {{2}},
Pages = {{135-147}},
Abstract = {{Traditional literary criticism concerns itself with metaphors in texts.
   There is a long tradition, however, of illustrated literature-texts with
   accompanying pictures. This article examines the relationship between
   pictures and text in the works of William Makepeace Thackeray, a
   19th-century English novelist who illustrated his own works and whose
   pictures are more than merely decorative. They provide visual
   explanation and ironic commentary on the text through a subtle system of
   related metaphors. The metaphors in the illustrations range from simple
   to highly complex, and close analysis shows them to be an integral part
   of the narrative, adding depth and meaning.}},
