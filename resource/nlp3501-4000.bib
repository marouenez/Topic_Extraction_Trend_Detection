@article{ ISI:000412439400015,
Author = {Kloekhorst, Alwin},
Title = {{The Old Hittite and the Proto-Indo-European tense-aspect system}},
Journal = {{INDOGERMANISCHE FORSCHUNGEN}},
Year = {{2017}},
Volume = {{122}},
Number = {{1}},
Pages = {{295-307}},
Month = {{SEP}},
Abstract = {{Building on Melchert's observation that in Old Hittite narrative texts
   an aspectual system seems to be used that is different from the
   aspectual system found in the majority of Hittite texts (Melchert 1998),
   this article discusses the nature of the tense-aspect system of
   Proto-Indo-European, especially on the basis of a comparison between the
   Sanskrit and Hittite verbal systems (following observations and analyses
   by Pooth 2009). It is argued that the tense-aspect system of the Old
   Hittite narrative texts as described by Melchert may go back directly to
   the system that can be reconstructed for early Proto-Indo-European,
   namely one in which there was no distinction in tense, but rather a
   distinction between progressive and non-progressive aspect.}},
