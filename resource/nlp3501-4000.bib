@article{ ISI:000261203200014,
Author = {Prieto, Javier Villoria},
Title = {{Piferrer natural methods: a pioneer in the teaching of English
   pronunciation}},
Journal = {{PORTA LINGUARUM}},
Year = {{2008}},
Number = {{9}},
Pages = {{199-218}},
Month = {{JAN}},
Abstract = {{This paper, a historical descriptive study, is part of a ambitious
   research project that aims to know and analyse the methodology displayed
   in the teaching and learning process of English in the Spanish
   educational system from its origin up to the present time. It is carried
   out through textbooks that were used in the classrooms. We consider them
   a fundamental source to know the history of Education, in our case the
   history of the teaching of English pronunciation in Spain. These
   materials have been scarcely used in research, and our pourpose is to
   make the most of the potential they held. To carry out this study we use
   the work of the catalan teacher of languages Francisco Piferrer and
   analyse his transcription techniques, phonetic design and the
   methodology he used to teach the English pronunciation.}},
