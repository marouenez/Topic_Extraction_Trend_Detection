@article{ ISI:000389205700010,
Author = {Gonzalez-Blanco, Elena},
Title = {{Digital Scholarly Editions for: Some Approaches and Future Perspectives}},
Journal = {{RILCE-REVISTA DE FILOLOGIA HISPANICA}},
Year = {{2017}},
Volume = {{33}},
Number = {{1}},
Pages = {{239-258}},
Abstract = {{Digital scholarly edition is conditioned by a continuous and challenging
   process of change due to the availability of many different technical
   solutions. However, those theoretical possibilities face with the real
   situation of editions in Spain, where the so-called ``digital gap{''}
   grows constantly. This paper deals with the analysis of the
   international panorama of resources, tools and projects related to
   digital scholarly editions. Its objective is to study their ways of
   evolution and other relevant aspects, as open access, friendly
   interfaces, accessibility, standardization level, interdisciplinary work
   and interoperability. The final aim of these analysis and reflections is
   to offer new guidelines and perspectives for the future of the text,
   taking it further than a simple study object.}},
