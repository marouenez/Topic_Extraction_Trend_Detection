@article{ ISI:000172263400094,
Author = {Mendonca, EA and Cimino, JJ and Johnson, SB},
Title = {{Using narrative reports to support a digital library}},
Journal = {{JOURNAL OF THE AMERICAN MEDICAL INFORMATICS ASSOCIATION}},
Year = {{2001}},
Number = {{S}},
Pages = {{458-462}},
Note = {{Annual Symposium of the American-Medical-Informatics-Association (AMIA
   2001), WASHINGTON, D.C., NOV 03-07, 2001}},
Organization = {{Amer Med Informat Assoc}},
Abstract = {{The vast amount of information collected and stored in clinical systems
   can be a significant challenge in the integration of digital libraries
   and electronic medical records, especially the selection of clinical
   data to be used in the search, retrieval, and summarization processes.
   In this study, we describe the use of information retrieval measures
   with natural language processor output to identify critical information
   in narrative reports. Our hypothesis is that clinical data that occur
   often in narrative reports are less important to clinicians than
   findings that occur rarely. We used the information retrieval methods to
   analyze one year of discharge summaries. Me then conducted a performance
   study, using physicians as subject. Results show that the methods can be
   used for filtering critical information from reports. Further studies
   need to be done on evaluation of the method based on an evaluation of
   the system performance in the context of a digital library.}},
