@article{ ISI:000369899300005,
Author = {Vivas Marquez, Julia and Ridao Rodrigo, Susana},
Title = {{(Im)politeness strategies in social networks: A comparative analysis of
   Facebook and Twitter}},
Journal = {{SINTAGMA}},
Year = {{2015}},
Volume = {{27}},
Pages = {{73-87}},
Abstract = {{The aim of this paper is to analyze the (im)politeness present in two
   popular social networks in Spain, namely Facebook and Twitter, using
   both quantitative and qualitative comparative studies. To do this,
   comments made to a publication that contains exactly the same text that
   was written by the same journalist are analyzed, firstly, on his
   Facebook profile, and, secondly, on his Twitter profile. In conclusion,
   a wide use of impoliteness in both social networks has been observed;
   more specifically, the discursive categories of impoliteness reach
   70.66\% of the corpus on Facebook, and 60.66\% on Twitter, and the
   discourse on Facebook is generally found to be rather more offensive.}},
