@article{ ISI:000337163500011,
Author = {Bei Xu and Hai Zhuge},
Title = {{Faceted navigation through keyword interaction}},
Journal = {{WORLD WIDE WEB-INTERNET AND WEB INFORMATION SYSTEMS}},
