@article{ ISI:000313016300008,
Author = {Gowanlock, Michael and Gazan, Rich},
Title = {{Assessing researcher interdisciplinarity: a case study of the University
   of Hawaii NASA Astrobiology Institute}},
Journal = {{SCIENTOMETRICS}},
Year = {{2013}},
Volume = {{94}},
Number = {{1}},
Pages = {{133-161}},
Month = {{JAN}},
Abstract = {{In this study, we combine bibliometric techniques with a machine
   learning algorithm, the sequential information bottleneck, to assess the
   interdisciplinarity of research produced by the University of Hawaii
   NASA Astrobiology Institute (UHNAI). In particular, we cluster abstract
   data to evaluate Thomson Reuters Web of Knowledge subject categories as
   descriptive labels for astrobiology documents, assess individual
   researcher interdisciplinarity, and determine where collaboration
   opportunities might occur. We find that the majority of the UHNAI team
   is engaged in interdisciplinary research, and suggest that our method
   could be applied to additional NASA Astrobiology Institute teams in
   particular, or other interdisciplinary research teams more broadly, to
   identify and facilitate collaboration opportunities.}},
