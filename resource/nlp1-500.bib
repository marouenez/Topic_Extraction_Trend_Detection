@article{ ISI:000389336100008,
Author = {Huang, Youliang and Huang, Qian and Ali, Sajid and Zhai, Xing and Bi,
   Xiaoming and Liu, Renquan},
Title = {{Rehabilitation using virtual reality technology: a bibliometric
   analysis, 1996-2015}},
