@article{ ISI:000406497200004,
Author = {Yang, Chao and Zhu, Donghua and Wang, Xuefeng and Zhang, Yi and Zhang,
   Guangquan and Lu, Jie},
Title = {{Requirement-oriented core technological components' identification based
   on SAO analysis}},
Journal = {{SCIENTOMETRICS}},
