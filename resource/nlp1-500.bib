@article{ ISI:000376273700016,
Author = {Akhavan, Peyman and Ebrahim, Nader Ale and Fetrati, Mahdieh A. and
   Pezeshkan, Amir},
Title = {{Major trends in knowledge management research: a bibliometric study}},
