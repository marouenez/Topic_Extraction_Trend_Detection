@article{ ISI:000353505400002,
Author = {Zappavigna, Michele},
Title = {{Searchable talk: the linguistic functions of hashtags}},
Journal = {{Social Semiotics}},
Year = {{2015}},
Volume = {{25}},
Number = {{3}},
Pages = {{274-291}},
Month = {{MAY 27}},
Abstract = {{An important dimension of social media discourse is its searchability. A
   key semiotic resource supporting this function is the hashtag, a form of
   social tagging that allows microbloggers to embed metadata in social
   media posts. While popularly thought of as topic-markers, hashtags are
   able to construe a range of complex meanings in social media texts. This
   paper uses the concept of linguistic metafunctions, to explore how
   hashtags enact three simultaneous communicative functions: marking
   experiential topics, enacting interpersonal relationships, and
   organizing text. Corpus-based discourse analysis of linguistic patterns
   in a 100 million word Twitter corpus is used to investigate these
   functions and how they relate to the notion of social search.}},
