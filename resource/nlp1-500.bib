@article{ ISI:000348451100009,
Author = {Chen, Hongshu and Zhang, Guangquan and Zhu, Donghua and Lu, Jie},
Title = {{A patent time series processing component for technology intelligence by
   trend identification functionality}},
Journal = {{NEURAL COMPUTING \& APPLICATIONS}},
Year = {{2015}},
Volume = {{26}},
