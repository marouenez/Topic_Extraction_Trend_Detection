@article{ ISI:000298047700009,
Author = {Kreuzthaler, Markus and Bloice, Marcus D. and Faulstich, Lukas and
   Simonic, Klaus-Martin and Holzinger, Andreas},
Title = {{A Comparison of Different Retrieval Strategies Working on Medical Free
   Texts}},
Journal = {{JOURNAL OF UNIVERSAL COMPUTER SCIENCE}},
Year = {{2011}},
Volume = {{17}},
Number = {{7}},
Pages = {{1109-1133}},
Abstract = {{Patient information in health care systems mostly consists of textual
   data, and free text in particular makes up a significant amount of it.
   Information retrieval systems that concentrate on these text types have
   to deal with the different challenges these medical free texts pose to
   achieve an acceptable performance. This paper describes the evaluation
   of four different types of information retrieval strategies: keyword
   search, search performed by a medical domain expert, a semantic based
   information retrieval tool, and a purely statistical information
   retrieval method. The different methods are evaluated and compared with
   respect to its appliance in medical health care systems.}},
Publisher = {{GRAZ UNIV TECHNOLGOY, INST INFORMATION SYSTEMS COMPUTER MEDIA-IICM}},
Address = {{INFFELDGASSE 16C, GRAZ, A-8010, AUSTRIA}},
