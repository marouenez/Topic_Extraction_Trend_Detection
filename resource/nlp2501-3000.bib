@article{ ISI:000209569200011,
Author = {Ma, Qing},
Title = {{Matching vocabulary learning process with learning outcome in L2
   academic writing: An exploratory case study}},
Journal = {{LINGUISTICS AND EDUCATION}},
Year = {{2013}},
Volume = {{24}},
Number = {{2}},
Pages = {{237-246}},
Month = {{JUN}},
Abstract = {{This exploratory case study of two undergraduates links vocabulary
   learning approaches with lexical quality measured in academic writing.
   Employing an array of qualitative data, it is shown that in a
   ``semi-language-rich{''} learning context, Chinese learners may dispense
   with rote learning and engage in a more natural learning approach in
   which vocabulary items are discovered from reading newspapers and
   consolidated in writing. A shift in language assessment is another key
   factor that contributes to this rarely reported learning approach among
   Chinese learners. Measuring lexical quality shows that the two students
   made impressive but differentiated lexical achievement. Underlying these
   findings is the two students' highly explicit metacognitive awareness of
