@article{ ISI:000409308400005,
Author = {Macintyre, Peter D. and Baker, Susan C. and Sparling, Heather},
Title = {{Heritage Passions, Heritage Convictions, and the Rooted L2 Self: Music
   and Gaelic Language Learning in Cape Breton, Nova Scotia}},
Journal = {{MODERN LANGUAGE JOURNAL}},
