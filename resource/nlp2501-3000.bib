@article{ ISI:000243118500010,
Author = {Louwerse, Max and Cai, Zhiqiang and Hu, Xiangen and Ventura, Matthew and
   Jeuniaux, Patrick},
Title = {{Cognitively inspired NLP-based knowledge representations: Further
   explorations of Latent semantic analysis}},
