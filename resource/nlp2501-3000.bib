@article{ ISI:000261303800007,
Author = {Sandor, Agnes},
Title = {{Modeling metadiscourse conveying the author's rhetorical strategy in
   biomedical research abstracts}},
Journal = {{REVUE FRANCAISE DE LINGUISTIQUE APPLIQUEE}},
Year = {{2007}},
Volume = {{12}},
Number = {{2}},
Pages = {{97-108}},
Month = {{DEC}},
Abstract = {{The importance of the role of metadiscourse is increasingly recognized
   for natural language processing applications like text-mining and
   information extraction. Thus the detection of metadiscourse has recently
   been identified as a task in several domains, including the processing
   of scientific literature. We have developed a natural language
   processing system that detects and highlights in biomedical research
   abstracts a particular kind of metadiscourse that conveys the author's
   rhetorical strategy. In this paper we describe the model of rhetorical
   metadiscourse underlying the system. Our model, combining and extending
   previous discourse analysis methods and models, is based on both
   conceptual and syntactic analyses of metadiscourse. We argue that this
   model is effective for automatic processing.}},
