@article{ ISI:A1993LL39900003,
Author = {CUTLER, A},
Title = {{PHONOLOGICAL CUES TO OPEN-CLASS AND CLOSED-CLASS WORDS IN THE PROCESSING
   OF SPOKEN SENTENCES}},
Journal = {{JOURNAL OF PSYCHOLINGUISTIC RESEARCH}},
Year = {{1993}},
Volume = {{22}},
Number = {{2}},
Pages = {{109-131}},
Month = {{MAR}},
Abstract = {{Evidence is presented that (a) the open and the closed word classes in
   English have different phonological characteristics, (b) the
   phonological dimension on which they differ is one to which listeners
   are highly sensitive, and (c) spoken open- and closed-class words
   produce different patterns of results in some auditory recognition
   tasks. What implications might link these findings? Two recent lines of
   evidence from disparate paradigms-the learning of an artificial
   language, and natural and experimentally induced misperception of
   Juncture-are summarized, both of which suggest that listeners are
   sensitive to the phonological reflections of open- vs. closed-class word
   status. Although these correlates cannot be strictly necessary for
   efficient processing, if they are present listeners exploit them in
   making word class assignments. That such a use of phonological
   information is of value to listeners could be indirect evidence that
   open- vs. closed class words undergo different processing operations}},
