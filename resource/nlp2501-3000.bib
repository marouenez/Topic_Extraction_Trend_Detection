@article{ ISI:000261303900006,
Author = {Picton, Aurelie and Fabre, Cecile and Bourigault, Didier},
Title = {{Linguistic methods for the expanion of requests.<bold> </bold>an
   experiment based on the use of distributional neighborhood}},
Journal = {{REVUE FRANCAISE DE LINGUISTIQUE APPLIQUEE}},
Year = {{2008}},
Volume = {{13}},
Number = {{1}},
Pages = {{83-95}},
Month = {{JUN}},
Abstract = {{This paper reports the results of a query expansion experiment making
   use of semantic data produced by a program that automatically performs
   the distributional analysis of a large corpus. This method allows us to
   take into account relations that go beyond classic lexical relations. We
   look at these results from both a global and a local perspective,
   showing that such broader and corpus-based semantic relations may be
   useful, provided that the expansion is controlled by a filtering
   technique based on the texts of the database (distributional feedback),
   and by an analysis of the query linguistic characteristics.}},
