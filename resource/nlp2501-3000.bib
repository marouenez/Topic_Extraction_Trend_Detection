@article{ ISI:000369591600008,
Author = {Isaki, Emi and Brown, Betty G. and Aleman, Sara and Hackstaff, Karla},
Title = {{Therapeutic Writing An Exploratory Speech-Language Pathology Counseling
   Technique}},
Journal = {{TOPICS IN LANGUAGE DISORDERS}},
Year = {{2015}},
Volume = {{35}},
Number = {{3}},
Pages = {{275-287}},
Month = {{JUL-SEP}},
Abstract = {{This exploratory qualitative study investigated the use of therapeutic
   writing for counseling long-term caregivers of spouses with brain injury
   and neurogenic communication disorders. Three participants wrote an
   average of six single-spaced pages of text. After analysis of the
   written text, the common themes of onset of diagnosis, anger, grief, and
   similarities in coping mechanisms were identified. Additional
   information about the value of therapeutic writing was obtained.
   Therapeutic writing appears to be a promising technique to use for
   counseling caregivers. On the basis of the caregiver's written text, the
   counseling needs related to neurogenic communication disorders can be
   addressed more efficiently.}},
