@article{ ISI:A1994QB41700002,
Author = {INGRAM, JL},
Title = {{THE ROLE OF FIGURATIVE LANGUAGE IN PSYCHOTHERAPY - A METHODOLOGICAL
   EXAMINATION}},
Journal = {{METAPHOR AND SYMBOLIC ACTIVITY}},
Year = {{1994}},
Volume = {{9}},
Number = {{4}},
Pages = {{271-288}},
Abstract = {{This investigation was undertaken to identify the role of figurative
   language in a single session of psychotherapy.  To accomplish this task,
   a method of structured content analysis was developed, which was
   effective in clarifying the thematic structure of the session.  This
   structure was then used to identify specific functions of metaphor in
   the text.  This analysis offers clear evidence for the central role of
   metaphor in encompassing and explicating key issues in psychotherapy. 
   The content of the session also provided evidence of distinct roles for
   novel and frozen figures of speech.}},
