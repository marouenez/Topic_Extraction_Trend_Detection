@article{ ISI:000286009000002,
Author = {Urena Gomez-Moreno, Jose Manuel and Faber, Pamela},
Title = {{Strategies for the Semi-Automatic Retrieval of Metaphorical Terms}},
Journal = {{METAPHOR AND SYMBOL}},
Year = {{2011}},
Volume = {{26}},
Number = {{1}},
Pages = {{23-52}},
Abstract = {{This article proposes a method for the semi-automatic extraction of
   resemblance metaphor terms from a manually annotated corpus of marine
   biology texts in English and Spanish. The corpus was first searched for
   target domain terms as well as for lexical markers indicative of
   metaphors. The combination of these search strategies for metaphor
   extraction resulted in a set of English-Spanish term pairs. After
   analysing and comparing these metaphor candidates, a quantitative
   analysis provided comparative statistical data regarding marine biology
   metaphor. Finally, the metaphorical nature of marine biology terms was
   verified in three ways. The first verification strategy entailed an
   adapted version of the Metaphor Identification Procedure (Pragglejaz
   Group, 2007). The second involved the analysis of contextual data
   extracted from the corpus, and the third involved the analysis of visual
   images from an online marine biology database and from the Google search
   engine.}},
