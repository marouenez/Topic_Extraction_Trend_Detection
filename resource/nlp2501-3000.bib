@article{ ISI:000287710000005,
Author = {alZahir, Saif and Borici, Arber},
Title = {{Entropy-based Assessment of Written Albanian Language}},
Journal = {{JOURNAL OF QUANTITATIVE LINGUISTICS}},
Year = {{2011}},
Volume = {{18}},
Number = {{1}},
Pages = {{89-106}},
Abstract = {{The entropy analysis of a printed language is vital for a wide range of
   applications, such as compression, written text recognition, text
   processing, and encryption. In this paper, we present empirical results
   on the entropy of written Albanian language based on a frequency
   analysis of letters, digrams, trigrams, and for orthographic words of
   length one, two and three characters. This work represents the first
   attempt at calculating some statistics for the Albanian language. Our
   sample consists of more than 1.86 million letters with a total of
   433,366 words obtained electronically from various sources-newspapers,
   journals, books, and websites-and comes from different applications such
   as business, politics, health, religion, sports, literature, and
   history. Because the real probability distribution of Albanian language
   n-grams is not known, we consider our calculated (trigram) entropy of
   3.2972 as an upper bound estimate for the true entropy of the Albanian
   language. We established that words of one, two, and three characters
   represent nearly 43\% of the total language volume, which is comparable
   to the English language.}},
