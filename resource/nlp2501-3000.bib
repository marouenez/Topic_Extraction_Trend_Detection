@article{ ISI:000074259500003,
Author = {Wallis, DA},
Title = {{Language, attitude, and ideology: An experimental social-psychological
   study}},
Journal = {{JOURNAL OF PRAGMATICS}},
Year = {{1998}},
Volume = {{30}},
Number = {{1}},
Pages = {{21-48}},
Month = {{JUL}},
Abstract = {{This study utilizes Pecheux's method of automatic discourse analysis
   (AAD) to analyze English texts produced under experimental conditions.
   The independent variable, attitude, was hypothesized to determine the
   content and form of inter-individual ideological discourse structure.
   Details of the AAD procedure for use with English texts are provided, as
   well as a theoretical discussion of the importance of Pecheux's
   meta-theory of language and ideology. This study is important as the
   first and only application of Pecheux's method to English texts, as well
   as for validating an earlier Pecheux study.}},
